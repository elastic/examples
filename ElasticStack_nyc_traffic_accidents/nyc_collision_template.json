{
    "template" : "nyc_visionzero",
    "settings" : {
      "index.refresh_interval" : "5s"
    },
    "mappings" : {
      "_default_" : {
        "dynamic_templates" : [ {
          "string_fields" : {
            "mapping" : {
              "type" : "text",
              "omit_norms" : true,
              "fields" : {
                "raw" : {
                  "type" : "keyword",
                  "ignore_above" : 256
                }
              }
            },
            "match_mapping_type" : "string",
            "match" : "*"
          }
        } ],
        "properties" : {
          "coords" : {
            "type" : "geo_point"
          },
          "@version" : {
            "type" : "keyword"
          },
          "borough": {
          	"type" : "keyword"
          },
          "cross_street_name" :{
          	"type" : "keyword"
          },
          "off_street_name":{
          	"type" : "keyword"
          },
          "on_street_name":{
          	"type" : "keyword"
          },
          "contributing_factor_vehicle":{
          	"type" : "keyword"
          },
          "intersection":{
            "type" : "keyword"
          },
          "hour_of_day": {
            "type" : "integer"
          }
        },
        "_all" : {
          "enabled" : true
        }
      }
    },
    "aliases" : { }
}
